{
  "name": "exotic-pets-ecommerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node api/server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "vite build",
    "build:cloudfront": "vite build && node scripts/optimize-for-cloudfront.js",
    "deploy:help": "echo 'Uso: npm run deploy:s3 -- --bucket=tu-bucket --distribution=tu-distribution-id'",
    "deploy:s3": "npm run build:cloudfront && powershell -ExecutionPolicy Bypass -File scripts/deploy-to-s3.ps1",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "npm run build && npm run preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "aws-amplify": "^6.15.7",
    "contentful": "^11.7.15",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "vite": "^7.0.4"
  }
}
